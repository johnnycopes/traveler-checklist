{
  "rules": {
		"eofline": true,
		"indent": [true, "tabs", 2],
		"interface-name": [true, "always-prefix"],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
		},
		"quotemark": [true, "single"]
  }
}